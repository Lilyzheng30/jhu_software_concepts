# Optional admin/bootstrap user for one-time DB creation
DB_ADMIN_HOST=127.0.0.1
DB_ADMIN_PORT=5432
DB_ADMIN_NAME=postgres
DB_ADMIN_USER=postgres
DB_ADMIN_PASSWORD=replace_me
